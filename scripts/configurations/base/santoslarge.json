{
    "general": {
        "test_name": "base",
        "datalake_config": {
            "location": "/path/to/data_lake",
            "name": "santoslarge",
            "options": [
                "/path/to/mapping_id_file",
                "/path/to/numeric_columns_file"
            ]
        },
        "blacklist": [],
        "k": 20,
        "num_query_samples": 100,
        "num_cpu": 72,
        "token_translators": [
            "whitespace", "punctuation", "lowercase"
        ],
        
        "josie_db_connection_info": {
            "drivername":   "postgresql",
            "database":     "DLTFSantosLarge",
            "username":     "nanni",
            "password":     "",
            "port":         5442,
            "host":         "127.0.0.1"
        }
    },
    
    "data_preparation": {
        "algorithms": [
            "josie", "lshforest", "embedding"
        ],
        "modes": [
            "set", "bag", "ft"
        ],
        "spark_config": {
            "spark.app.name":               "JOSIE Data Preparation",
            "spark.master":                 "local[64]",
            "spark.executor.memory":        "100g",
            "spark.driver.memory":          "20g",
            "spark.local.dir":              "/path/to/.tmp_spark",
            "spark.driver.maxResultSize":   "12g",
            "spark.jars.packages":          "org.mongodb.spark:mongo-spark-connector_2.12:10.3.0",
            "spark.driver.extraClassPath":  "/path/to/.ivy2/jars/org.postgresql_postgresql-42.7.3.jar"
        },
        "token_table_on_mem": false,
        "num_perm": 256,
        "l": 32,
        "exec_now": false
    },

    "sample_queries": {
        "exec_now": false
    },

    "query": {
        "algorithms": [
            "jose", "lshforest", "embedding"
        ],
        "modes": [
            "set", "bag", "ft"
        ],
        "exec_now": false
    },

    "extract": {
        "clear_results_table": false,
        "exec_now": false
    },

    "analyses": {
        "p_values": [
            1, 5, 10, 20
        ],
        "exec_now": false
    },

    "clean": {
        "algorithms": [
            "josie", "lshforest", "embedding"
        ],
        "modes": [
            "set", "bag", "ft", "cft"
        ],
        "exec_now": false
    }
}